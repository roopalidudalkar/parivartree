<?php

namespace Tree\TreeBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Eventtoaddressrel
 */
class Eventtoaddressrel
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $eventid;

    /**
     * @var string
     */
    private $housenumber;

    /**
     * @var string
     */
    private $streetnumber;

    /**
     * @var integer
     */
    private $pincode;

    /**
     * @var string
     */
    private $location;

    /**
     * @var string
     */
    private $city;

    /**
     * @var string
     */
    private $state;

    /**
     * @var string
     */
    private $country;

    /**
     * @var integer
     */
    private $geomap;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set eventid
     *
     * @param integer $eventid
     * @return Eventtoaddressrel
     */
    public function setEventid($eventid)
    {
        $this->eventid = $eventid;

        return $this;
    }

    /**
     * Get eventid
     *
     * @return integer 
     */
    public function getEventid()
    {
        return $this->eventid;
    }

    /**
     * Set housenumber
     *
     * @param string $housenumber
     * @return Eventtoaddressrel
     */
    public function setHousenumber($housenumber)
    {
        $this->housenumber = $housenumber;

        return $this;
    }

    /**
     * Get housenumber
     *
     * @return string 
     */
    public function getHousenumber()
    {
        return $this->housenumber;
    }

    /**
     * Set streetnumber
     *
     * @param string $streetnumber
     * @return Eventtoaddressrel
     */
    public function setStreetnumber($streetnumber)
    {
        $this->streetnumber = $streetnumber;

        return $this;
    }

    /**
     * Get streetnumber
     *
     * @return string 
     */
    public function getStreetnumber()
    {
        return $this->streetnumber;
    }

    /**
     * Set pincode
     *
     * @param integer $pincode
     * @return Eventtoaddressrel
     */
    public function setPincode($pincode)
    {
        $this->pincode = $pincode;

        return $this;
    }

    /**
     * Get pincode
     *
     * @return integer 
     */
    public function getPincode()
    {
        return $this->pincode;
    }

    /**
     * Set location
     *
     * @param string $location
     * @return Eventtoaddressrel
     */
    public function setLocation($location)
    {
        $this->location = $location;

        return $this;
    }

    /**
     * Get location
     *
     * @return string 
     */
    public function getLocation()
    {
        return $this->location;
    }

    /**
     * Set city
     *
     * @param string $city
     * @return Eventtoaddressrel
     */
    public function setCity($city)
    {
        $this->city = $city;

        return $this;
    }

    /**
     * Get city
     *
     * @return string 
     */
    public function getCity()
    {
        return $this->city;
    }

    /**
     * Set state
     *
     * @param string $state
     * @return Eventtoaddressrel
     */
    public function setState($state)
    {
        $this->state = $state;

        return $this;
    }

    /**
     * Get state
     *
     * @return string 
     */
    public function getState()
    {
        return $this->state;
    }

    /**
     * Set country
     *
     * @param string $country
     * @return Eventtoaddressrel
     */
    public function setCountry($country)
    {
        $this->country = $country;

        return $this;
    }

    /**
     * Get country
     *
     * @return string 
     */
    public function getCountry()
    {
        return $this->country;
    }

    /**
     * Set geomap
     *
     * @param integer $geomap
     * @return Eventtoaddressrel
     */
    public function setGeomap($geomap)
    {
        $this->geomap = $geomap;

        return $this;
    }

    /**
     * Get geomap
     *
