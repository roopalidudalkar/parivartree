<?php
// src/Tree/TreeBundle/Entity/Syslog.php

namespace Tree\TreeBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

class Syslog
{

    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $log_type;

    /**
     * @var string
     */
    private $activity_message;

    /**
     * @var integer
     */
    private $log_level;

    /**
     * @var \DateTime
     */
    private $log_time;

    /**
     * @var integer
     */
    private $log_ip;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set log_type
     *
     * @param integer $logType
     * @return Syslog
     */
    public function setLogType($logType)
    {
        $this->log_type = $logType;

        return $this;
    }

    /**
     * Get log_type
     *
     * @return integer 
     */
    public function getLogType()
    {
        return $this->log_type;
    }

    /**
     * Set activity_message
     *
     * @param string $activityMessage
     * @return Syslog
     */
    public function setActivityMessage($activityMessage)
    {
        $this->activity_message = $activityMessage;

        return $this;
    }

    /**
     * Get activity_message
     *
     * @return string 
     */
    public function getActivityMessage()
    {
        return $this->activity_message;
    }

    /**
     * Set log_level
     *
     * @param integer $logLevel
     * @return Syslog
     */
    public function setLogLevel($logLevel)
    {
        $this->log_level = $logLevel;

        return $this;
    }

    /**
     * Get log_level
     *
     * @return integer 
     */
    public function getLogLevel()
    {
        return $this->log_level;
    }

    /**
     * Set log_time
     *
     * @param \DateTime $logTime
     * @return Syslog
     */
    public function setLogTime($logTime)
    {
        $this->log_time = $logTime;

        return $this;
    }

    /**
     * Get log_time
     *
     * @return \DateTime 
     */
    public function getLogTime()
    {
        return $this->log_time;
    }

    /**
     * Set log_ip
     *
     * @param integer $logIp
     * @return Syslog
     */
    public function setLogIp($logIp)
    {
        $this->log_ip = $logIp;

        return $this;
    }

    /**
     * Get log_ip
     *
     * @return integer 
     */
    public function getLogIp()
    {
        return $this->log_ip;
    }
}
