{% extends '::base.html.twig' %}




{% block body %} 
<div class="navbar navbar-default " role="navigation">
      <div class="container-fluid container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Parivar Tree</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="{{ path('admin_admin_dashboard') }}">Dashboard</a></li>
            <li><a href="{{ path('admin_admin_userdetails') }}">User Data</a></li>
            <li><a href="#">Profile</a></li>
            <li><a href="#">Help</a></li>
          </ul>
          <form class="navbar-form navbar-right">
            <input type="text" class="form-control" placeholder="Search...">
          </form>
        </div>
      </div>
    </div>
	
	
<div class="container">	
<div class="table-responsive">

<table class="table table-bordered">
	
	<tr>
		<td><strong>Firstname</strong></td>
		<td><strong>Lastname</strong></td>
		<td><strong>Email</strong></td>
		<td><strong>Mobile No</strong></td>
		<td><strong>Created Date</strong></td>
		<td><strong>Updated Date</strong></td>
		<td><strong>Last Accessed<?strong></td>
		<td><strong>Last Accessed IP</strong></td>
		<td><strong>Status</strong></td>
	</tr>
	{% for c in userdata %}
	<tr>
		<td>{{ c.firstname }}</td>
		<td>{{ c.lastname }}</td>
		<td><a href="{{ path('admin_admin_userprofile',{'email':c.email}) }}">{{ c.email }}</a></td>
		<td>{{ c.mobile1 }}</td>
		<td>{{ c.created | date('Y-m-d h:i:s') }}</td>
		<td>{{ c.updated | date('Y-m-d h:i:s') }}</td>
		<td>{{ c.last_access | date('Y-m-d h:i:s') }}</td>
		<td>{{ c.last_access_ip }}</td>
		<td width="30%">{% if c.status == 0 %} Inactive {% elseif c.status == 1 %} Active {% endif %}</td>
		<td><a href="{{ path('admin_admin_updateuser',{'email':c.email}) }}">Edit</a> | <a href="{{ path('admin_admin_deleteuser',{'email':c.email}) }}">Delete</a></td>
	</tr>
	
	{% endfor %}
</table>
</div>
</div>
{% endblock %}





